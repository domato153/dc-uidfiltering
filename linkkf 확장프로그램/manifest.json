{
  "manifest_version": 3,
  "name": "Linkkf 위치 이동 단축키",
  "version": "1.0",
  "description": "S키를 눌러 영상의 특정 위치(33%, 66%, 90%)로 이동합니다.",
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["controls.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}